    "directory": "dist"
  },
  "repository": "https://github.com/queelag/web-components",
  "scripts": {
    "build": "node scripts/build.mjs",
    "prepublish": "vitest run && node scripts/build.mjs",
    "test": "pnpm run /^test:.*/",
    "test:integration": "vitest run --config=vitest.browser.config.ts",
    "test:unit": "vitest run --config=vitest.config.ts",
    "storybook": "storybook dev -p 6006"
  },
  "sideEffects": false,
  "type": "module",
  "types": "index.d.ts",
